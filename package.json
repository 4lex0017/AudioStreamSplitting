{
  "name": "audio_streaming_splitting",
  "private": true,
  "packageManager": "npm@9.5.1",
  "scripts": {
    "fe": "npm run vite -- --open",
    "fe:test:unit": "vitest -c src/frontend/vite.config.ts",
    "be": "python3 src/backend/main.py --alone",
    "be:debug": "python3 src/backend/main.py --alone --debug",
    "view:app": "npm run build:fe && python3 src/backend/main.py",
    "view:app:debug": "npm run build:fe && python3 src/backend/main.py --debug",
    "build:fe": "npm run typecheck && npm run vite build",
    "build:app": "npm run build:fe && pyinstaller build.spec",
    "preview": "dist/app",
    "vite": "vite -c src/frontend/vite.config.ts",
    "lint": "eslint src/frontend --fix",
    "typecheck": "vue-tsc -p src/frontend --noEmit",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-final-modal": "^4.4.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@iconify-json/carbon": "^1.1.18",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@types/node": "^20.4.2",
    "@unocss/eslint-config": "^0.53.5",
    "@unocss/reset": "^0.53.5",
    "@unocss/transformer-directives": "^0.53.5",
    "@unocss/transformer-variant-group": "^0.53.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.45.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.11.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run typecheck"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ],
    "rules": {
      "no-console": "off"
    }
  }
}
