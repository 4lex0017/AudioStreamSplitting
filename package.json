{
  "name": "audio_streaming_splitting",
  "private": true,
  "scripts": {
    "fe": "npm run vite",
    "fe:test": "vitest",
    "be": "python3 src/backend/main.py --alone",
    "be:debug": "python3 src/backend/main.py --alone --debug",
    "view:app": "npm run build:fe && python3 src/backend/main.py",
    "view:app:debug": "npm run build:fe && python3 src/backend/main.py --debug",
    "build:fe": "npm run typecheck && npm run vite build",
    "build:app": "npm run build:fe && pyinstaller build.spec",
    "preview": "dist/app",
    "vite": "vite -c src/frontend/vite.config.ts",
    "lint": "eslint src/frontend --fix",
    "typecheck": "vue-tsc -p src/frontend --noEmit",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@iconify-json/carbon": "^1.1.17",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/node": "^20.2.5",
    "@unocss/eslint-config": "^0.52.7",
    "@unocss/reset": "^0.52.7",
    "@unocss/transformer-directives": "^0.52.7",
    "@unocss/transformer-variant-group": "^0.52.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.41.0",
    "jsdom": "^22.0.0",
    "taze": "^0.10.1",
    "typescript": "^5.0.4",
    "unocss": "^0.52.7",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vite-plugin-pages": "^0.30.1",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.6.5"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
