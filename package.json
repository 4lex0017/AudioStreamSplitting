{
  "name": "audio_streaming_splitting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "fe": "npm run vite",
    "be": "python3 src/backend/main.py --alone",
    "be:debug": "python3 src/backend/main.py --alone --debug",
    "view:app": "npm run build:fe && python3 src/backend/main.py",
    "view:app:debug": "npm run build:fe && python3 src/backend/main.py --debug",
    "build:fe": "vue-tsc -p src/frontend && npm run vite build",
    "build:app": "npm run build:fe && pyinstaller build.spec",
    "preview": "dist/app ",
    "vite": "vite -c src/frontend/vite.config.ts",
    "fe:lint": "eslint --ext .vue,.ts src/frontend --fix --ignore-path .gitignore",
    "fe:prettier": "prettier src/frontend --write",
    "fe:format": "npm run fe:prettier && npm run fe:lint"
  },
  "dependencies": {
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "pinia": "^2.1.3",
    "vue": "^3.3.2",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
